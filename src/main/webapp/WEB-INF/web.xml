<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4"
	xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>Parking</display-name>
	<context-param>
		<description>
		Vaadin production mode</description>
		<param-name>productionMode</param-name>
		<param-value>false</param-value>
	</context-param>
	<!--<servlet>
		<servlet-name>WikiImageProxy</servlet-name>
		<servlet-class>com.vaadin.demo.parking.WikiImageProxy</servlet-class>
	</servlet>-->
	<servlet>
		<servlet-name>Parking</servlet-name>
		<servlet-class>com.vaadin.demo.parking.ParkingServlet</servlet-class>
		
		<init-param>
			<param-name>UIProvider</param-name>
			<param-value>com.vaadin.demo.parking.ParkingUIProvider</param-value>
		</init-param>
	</servlet>
	<!--<servlet-mapping>
		<servlet-name>WikiImageProxy</servlet-name>
		<url-pattern>/IMG/*</url-pattern>
	</servlet-mapping>-->
	<servlet-mapping>
		<servlet-name>Parking</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>


	<mime-mapping>
		<extension>manifest</extension>
		<mime-type>text/cache-manifest</mime-type>
	</mime-mapping>

